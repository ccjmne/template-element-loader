{
  "name": "template-tag-loader",
  "version": "1.0.0",
  "description": "A Webpack loader that serves your html contents into actual <template> elements",
  "files": [
    "dist"
  ],
  "main": "dist/template-tag-loader.umd.js",
  "module": "dist/template-tag-loader.es.js",
  "exports": {
    ".": {
      "import": "./dist/template-tag-loader.es.js",
      "require": "./dist/template-tag-loader.umd.js"
    }
  },
  "scripts": {
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccjmne/template-tag-loader.git"
  },
  "keywords": [
    "webpack",
    "loader",
    "html",
    "template",
    "element",
    "template-element",
    "custom-element"
  ],
  "author": "Eric NICOLAS <ccjmne@gmail.com> (ccjmne)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ccjmne/template-tag-loader/issues"
  },
  "homepage": "https://github.com/ccjmne/template-tag-loader#readme",
  "dependencies": {
    "html-minifier-terser": "^7.0.0-beta.0"
  },
  "devDependencies": {
    "@types/html-minifier-terser": "^6.1.0",
    "@types/node": "^18.0.3",
    "@types/webpack": "^5.28.0",
    "vite": "^2.9.13"
  }
}
